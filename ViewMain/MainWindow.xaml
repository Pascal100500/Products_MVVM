<Window x:Class="ViewMain.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ViewMain"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>

        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Margin="10" >
            <Button Content="Техника" Width="200" Margin="10" Click="OpenElectronicsWindow" />
            <Button Content="Овощи" Width="200" Margin="10" Click="OpenVegetableWindow" />
        </StackPanel>
        
    </Grid>
</Window>
